package study;

public class Operator {

	public static void main(String[] args) {
		
		int num1 = 10;
		int num2 = 2;
		int result = 0;
		char operator = 'l';
		
		switch(operator) {
			case '+' : result = num1 + num2;
				break;
				
			case '-' : result = num1 - num2;
				break;	
			
			case '*' : result = num1 * num2;
				break;
			
			case '/' : result = num1 / num2;
				break;
			
			default : 			
				System.out.println("연산자를 잘못 입력했습니다.");
				break;
		}
		
		System.out.println(result);
	}
}

package hello;

public class HelloJava {

	public static void main(String[] args) {
		int num1 = 10;
		int num2 = 2;
		int result=0;
		char operator = '/';
		
		switch(operator) {
			case '+': result = num1 + num2;
				break;
			case '-': result = num1 - num2;
				break;
			case '*': result = num1 * num2;
				break;
			case '/': result = num1 / num2;
				break;
			default:
				System.out.println("사칙연산이 아닙니다.");		
		}
		System.out.println(result);
	}

}

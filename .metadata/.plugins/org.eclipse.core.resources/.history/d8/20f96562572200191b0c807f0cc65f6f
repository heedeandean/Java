package chapter2;

public class Variable1 {

	public static void main(String[] args) {
		int dan;
		int times;
		
		for(dan=2; dan<=8; dan++) {
			if(dan % 2 != 0)
				continue;
			for(times=1; times<=9; times++) {
				System.out.println(dan + "x" + times + "=" + (dan * times));
			}
		System.out.println();
				
		}
	
	}
}
